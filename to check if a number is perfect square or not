#include<stdio.h>
#include<math.h>
int main()
{
int a,b;
float c;
printf("Enter the number:\n");
scanf("%d",&a);
//we can take any integer value for a
b=sqrt(a);
c=sqrt(a);
if((c-b)==0)
{
    printf("This number is a perfect square");
    //It means that its square root is an integer
}
else
{
    printf("This number is not a perfect square");
    //It means that its square root is not an integer
}
return 0;
}
